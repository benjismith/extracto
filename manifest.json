{
  "manifest_version": 3,
  "name": "Extracto",
  "version": "1.2",
  "description": "Select and export content from web pages.",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Extracto"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
